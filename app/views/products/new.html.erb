<h1>Products#new</h1>

<div>
  <%= simple_form_for @product do |f| %>
    <%= f.error_notification %>
    <%= f.input :name, autofocus: true%>
    <%= f.association :category %>
    <%= f.input :description %>
    <%= f.input :city %>
    <%= f.input :price_per_day %>
    <%= f.button :submit %>
  <% end %>
</div>
